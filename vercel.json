{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@4.0.0"
    }
  },
  "routes": [
    { "src": "/redirect-tests/list\\.csv", "dest": "/api/redirect-tests/list?format=csv" },
    { "src": "/redirect-tests/list\\.json", "dest": "/api/redirect-tests/list?format=json" },
    { "src": "/redirect-tests/meta-1", "dest": "/api/redirect-tests/meta-1" },
    { "src": "/redirect-tests/301-1", "dest": "/api/redirect-tests/301-1" },
    { "src": "/redirect-tests/302-1", "dest": "/api/redirect-tests/302-1" },
    { "src": "/redirect-tests/307-1", "dest": "/api/redirect-tests/307-1" },
    { "src": "/redirect-tests/308-1", "dest": "/api/redirect-tests/308-1" },
    { "src": "/redirect-tests/js-1", "dest": "/api/redirect-tests/js-1" },
    { "src": "/redirect-tests/301-2", "dest": "/api/redirect-tests/301-2" },
    { "src": "/redirect-tests/302-2", "dest": "/api/redirect-tests/302-2" },
    { "src": "/redirect-tests/307-2", "dest": "/api/redirect-tests/307-2" },
    { "src": "/redirect-tests/308-2", "dest": "/api/redirect-tests/308-2" },
    { "src": "/redirect-tests/meta-to-301", "dest": "/api/redirect-tests/meta-to-301" },
    { "src": "/redirect-tests/js-to-302", "dest": "/api/redirect-tests/js-to-302" },
    { "src": "/redirect-tests/six-http", "dest": "/api/redirect-tests/six-http" },
    { "src": "/redirect-tests/a1", "dest": "/api/redirect-tests/a1" },
    { "src": "/redirect-tests/a2", "dest": "/api/redirect-tests/a2" },
    { "src": "/redirect-tests/a3", "dest": "/api/redirect-tests/a3" },
    { "src": "/redirect-tests/a4", "dest": "/api/redirect-tests/a4" },
    { "src": "/redirect-tests/a5", "dest": "/api/redirect-tests/a5" },
    { "src": "/redirect-tests/loop-start", "dest": "/api/redirect-tests/loop-start" },
    { "src": "/redirect-tests/loop-b", "dest": "/api/redirect-tests/loop-b" },
    { "src": "/redirect-tests/messy-1", "dest": "/api/redirect-tests/messy-1" },
    { "src": "/redirect-tests/messy-2", "dest": "/api/redirect-tests/messy-2" },
    { "src": "/redirect-tests/messy-3", "dest": "/api/redirect-tests/messy-3" },
    { "src": "/redirect-tests/messy-4", "dest": "/api/redirect-tests/messy-4" },
    { "src": "/redirect-tests/messy-5", "dest": "/api/redirect-tests/messy-5" },
    { "src": "/redirect-tests/target", "dest": "/api/redirect-tests/target" },
    { "src": "/api/test-redirect", "dest": "/api/test-redirect" },
    { "src": "/redirect-tests/(.*)", "dest": "/api/redirect-tests/$1" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "NODE_VERSION": "18.x"
  }
}
