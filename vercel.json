{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@4.0.0"
    }
  },
  "routes": [
    { "src": "/redirect-tests/list\\.csv", "dest": "/api/redirect-tests/list?format=csv" },
    { "src": "/redirect-tests/list\\.json", "dest": "/api/redirect-tests/list?format=json" },
    { "src": "/redirect-tests/(.*)", "dest": "/api/redirect-tests/test?type=$1" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "NODE_VERSION": "18.x"
  }
}
