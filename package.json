{
  "name": "redirectinator",
  "version": "2.0.0",
  "type": "module",
  "description": "Professional website redirection, status checker, and monitor - client-side tool for SEO professionals",
  "main": "index.html",
  "scripts": {
    "dev": "vite --port 3000 --strictPort",
    "server": "PORT=3001 node server.js",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "seo",
    "redirect",
    "url-checker",
    "website-monitor",
    "client-side",
    "tech-seo"
  ],
  "author": "Tech SEO",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.0",
    "vite": "^7.1.3"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "file-saver": "^2.0.5",
    "helmet": "^8.1.0",
    "idb": "^7.1.1",
    "lucide-react": "^0.294.0",
    "morgan": "^1.10.1",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.4.1",
    "postcss": "^8.5.6",
    "proxy-agent": "^6.5.0",
    "puppeteer": "^24.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.0.0"
  }
}
